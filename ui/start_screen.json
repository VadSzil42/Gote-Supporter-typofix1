{
  "namespace": "start",

  "preview_warning_swtich_toggle@gtb.swtich_toggle": {
    "$toggle_name": "preview_warning"
  },
  "preview_warning_swtich_toggle_c@gtb.swtich_toggle_c": {
    "$toggle_name": "preview_warning"
  },
  "preview_warning_swtich_toggle_close@gtb.close_swtich_toggle_esc": {
    "$toggle_name": "preview_warning"
  },


  "quit_icon": {
    "type": "image",
    "texture": "textures/ui/cancel",
    "layer": 3,
    "size": [ 13, 13 ]
  },

  "info_icon": {
    "type": "image",
    "texture": "textures/ui/info_icon",
    "layer": 3,
    "size": [ 16, 16 ]
  },

  "warning_icon": {
    "type": "image",
    "texture": "textures/ui/WarningGlyph",
    "layer": 3,
    "size": [ 13, 20 ]
  },

  "upper_online_buttons_panel/stacked_column/gamertag_pic_and_label": {
    "ignored": "$education_edition",
    "size": [ "100%c", 24 ],
    "controls": [
      {
        "spacer_0": {
          "type": "panel",
          "size": [ 10, 0 ]
        }
      },
      {
        "gamerpic_with_border": {
          "type": "panel",
          "size": [ 24, 24 ],
          "controls": [
            {
              "gamerpic": {
                "type": "image",
                "size": [ 22, 22 ],
                "offset": [ 0, 4 ],
                "layer": 1,
                "bindings": [
                  {
                    "binding_name": "#gamerpic_texture_path",
                    "binding_name_override": "#texture"
                  },
                  {
                    "binding_name": "#gamerpic_texture_location_type",
                    "binding_name_override": "#texture_file_system"
                  }
                ],
                "controls": [
                  {
                    "border_white@common.focus_border_white": {
                      "layer": 3,
                      "size": [ 24, 24 ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "spacer_2": {
          "type": "panel",
          "size": [ 5, 0 ]
        }
      },
      {
        "label_panel": {
          "type": "panel",
          "size": [ "fill", 24 ],
          "controls": [
            {
              "gamertag_label": {
                "type": "label",
                "color": "$main_header_text_color",
                "text": "#gamertag_label",
                "size": [ "default", 10 ],
                "offset": [ 0, 4 ],
                "shadow": true,
                "bindings": [
                  {
                    "binding_name": "#gamertag_label"
                  }
                ]
              }
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "(not #sign_in_visible)",
        "binding_name_override": "#visible"
      }
    ]
  },

  "upper_online_buttons_panel": {
    "bindings": []
  },


  "copyright2": {
    "type": "panel",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [ "100%", 10 ],
    "controls": [
      {
        "gote-supporter_version": {
          "type": "label",
          "color": "$main_header_text_color",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "size": [ "default", 10 ],
          "offset": [ 0, -12 ],
          "text": "Gote-Supporter by ©YukaYama0311",
          "layer": 1,
          "controls": [
            {
              "label_background": {
                "type": "image",
                "texture": "textures/ui/White",
                "anchor_from": "top_right",
                "anchor_to": "top_right",
                "offset": [ 3, -1 ],
                "alpha": 0.6,
                "color": [ 0, 0, 0 ],
                "size": [ "100% + 4px", "100% + 2px" ],
                "layer": -5
              }
            }
          ]
        }
      },
      {
        "minecraft_version": {
          "type": "label",
          "color": "$main_header_text_color",
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "size": [ "default", 10 ],
          "text": "Minecraft by %menu.copyright",
          "layer": 1,
          "controls": [
            {
              "label_background": {
                "type": "image",
                "texture": "textures/ui/White",
                "anchor_from": "top_right",
                "anchor_to": "top_right",
                "offset": [ 3, -1 ],
                "alpha": 0.6,
                "color": [ 0, 0, 0 ],
                "size": [ "100% + 4px", "100% + 2px" ],
                "layer": -5
              }
            }
          ]
        }
      }
    ]
  },
  "ver_texts": {
    "type": "panel",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [ "100%-2px", 10 ],
    "controls": [
      {
        "gote-supporter_version": {
          "type": "label",
          "color": "$main_header_text_color",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "size": [ "default", 10 ],
          "offset": [ 0, -12 ],
          "text": "('Gote-Supporter v' + $gote_ver)",
          "layer": 1,
          "controls": [
            {
              "label_background": {
                "type": "image",
                "texture": "textures/ui/White",
                "anchor_from": "top_right",
                "anchor_to": "top_right",
                "offset": [ 3, -1 ],
                "alpha": 0.6,
                "color": [ 0, 0, 0 ],
                "size": [ "100% + 4px", "100% + 2px" ],
                "layer": -5
              }
            }
          ]
        }
      },
      {
        "gote-minecraft_version": {
          "type": "label",
          "color": "$main_header_text_color",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "size": [ "default", 10 ],
          "text": "#text",
          "layer": 1,
          "bindings": [
            {
              "binding_name": "#version"
            },
            {
              "binding_type": "view",
              "source_property_name": "('Minecraft ' + #version)",
              "target_property_name": "#text"
            }
          ],
          "controls": [
            {
              "label_background": {
                "type": "image",
                "texture": "textures/ui/White",
                "anchor_from": "top_right",
                "anchor_to": "top_right",
                "offset": [ 3, -1 ],
                "alpha": 0.6,
                "color": [ 0, 0, 0 ],
                "size": [ "100% + 4px", "100% + 2px" ],
                "layer": -5
              }
            }
          ]
        }
      }
    ]
  },

  "text_panel": {
    "type": "panel",
    "size": [ "100%-2px", 10 ],
    "offset": [ 0, -1 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "controls": [
      {
        "copyright2@start.copyright2": {}
      },
      {
        "development_version@start.development_version": {
          "ignored": "$is_publish"
        }
      },
      {
        "ver_texts@start.ver_texts": {}
      }
    ]
  },

  "preview_warning_text": {
    "type": "stack_panel",
    "size": [ "100% - 2px", "default" ],
    "offset": [ 2, 0 ],
    "anchor_from":"top_left",
    "anchor_to":"top_left",
    "controls": [
      {
        "1": {
          "type": "label",
          "size": [ "100%", "default" ],
          "font_type": "smooth",
          "text": "gtb.previewWarning.honbun1"
        }
      },
      {
        "2": {
          "type": "label",
          "size": [ "100%", "default" ],
          "font_type": "smooth",
          "text": "gtb.previewWarning.honbun2"
        }
      },
      {
        "3": {
          "type": "label",
          "size": [ "100%", "default" ],
          "font_type": "smooth",
          "text": "gtb.previewWarning.honbun3"
        }
      },
      {
        "4": {
          "type": "label",
          "size": [ "100%", "default" ],
          "font_type": "smooth",
          "text": "gtb.previewWarning.honbun4"
        }
      }
    ]
  },

  "preview_warning_text_scroll@common.scrolling_panel": {
    "$show_background": false,
    "size": [ "100% - 4px", "100%" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [ 2, 0 ],
    
    "$scrolling_content": "start.preview_warning_text",
    "$scrolling_panel_size": [ "100%", "100%" ],
    "$scroll_size": [ "4px", "100% - 2px" ],
    "$always_handle_scrolling": true
  },

  "preview_warning_okbutton@start.preview_warning_swtich_toggle": {
    "$checked_control": "common_toggles.toggle_unchecked",
    "$checked_hover_control": "common_toggles.toggle_unchecked_hover",

    "$button_text": "gui.ok",
    "$toggle_default_state": true,
    "$toggle_group_forced_index": 0,

    "$focus_override_down": "FOCUS_OVERRIDE_STOP",
    "$focus_override_up": "FOCUS_OVERRIDE_STOP",
    "$focus_override_left": "FOCUS_OVERRIDE_STOP",
    "$focus_override_right": "FOCUS_OVERRIDE_STOP"
  },

  "dialog1@gtb.dummy_dialog_one": {
    "layer": 503,
    "size": [ "60%", "60%" ],
    "$text_name": "gtb.previewWarning.title",
    "$child_control": "start.preview_warning_text_scroll",
    "$bottom_button_panel": "start.preview_warning_okbutton",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "preview_warning",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },
  
  "lower_online_buttons_panel/stacked_column": {
    "modifications": [
      {
        "control_name": "achievements_btn",
        "operation": "insert_after",
        "value": [
          {
            "pack_info_padding": {
              "type": "panel",
              "size": [ 3, "100%" ]
            }
          },
          {
            "pack_info_btn@start.column_frame": {
              "size": [ 28, 32 ],
              "controls": [
                {
                  "pack_info_icon_button": {
                    "type": "panel",
                    "$button_tts_header": "manifestvalidation.title",
                    "controls": [
                      {
                        "pack_info_button@common_buttons.light_content_button": {
                          "size": [ 24, 24 ],
                          "$button_content": "start.info_icon",
                          "$pressed_button_name": "button.menu_buy_game"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },
  
  "start_screen_content": {
    "modifications": [
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [
          {
            "preview_warning_button@start.preview_warning_swtich_toggle_c": {
              "ignored": "(not $pre_release or $education_edition)",
              "size": [ 25, 25 ],
              "offset": [ -5, 5 ],
              "anchor_from": "top_right",
              "anchor_to": "top_right",
              "$button_content": "start.warning_icon",
              "$toggle_view_binding_name": "preview_warning",
              "$toggle_group_forced_index": 100
            }
          },
          {
            "preview_warning_popup": {
              "type": "panel",
              "ignored": "(not $pre_release or $education_edition)",
              "size": [ "100%", "100%" ],
              "controls": [
                {
                  "scrool@common.scrolling_panel": {
                    "type": "scroll_view",
                    "size": [ "100%", "100%" ],
                    "layer": 501,
                    "$scrolling_content": "common.empty_panel",
                    "$show_background": false
                  }
                },
                {
                  "preview_warning_swtich_toggle_close@start.preview_warning_swtich_toggle_close": {
                    "layer": 502,
                    "size": [ "100%", "100%" ],
                    "$toggle_default_state": true,
                    "$toggle_group_forced_index": 0
                  }
                },
                {
                  "dialog1@start.dialog1": {}
                }
              ],
              "bindings": [
                {
                  "binding_type": "view",
                  "source_control_name": "preview_warning",
                  "source_property_name": "#toggle_state",
                  "target_property_name": "#visible"
                }
              ]
            }
          }
        ]
      }
    ]
  }
}