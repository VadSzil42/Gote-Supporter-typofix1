{
  "namespace": "tabbed_upsell",

  "warning_icon": {
    "type": "image",
    "texture": "textures/ui/ErrorGlyph",
    "layer": 3,
    "size": [ 13, 20 ]
  },



  "indexs_panel": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ]
  },

  "index_toggle_button_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "$button_label|default": "",
    "$button_icon|default": "textures/blocks/stone",
    "controls": [
      {
        "labels": {
          "type": "stack_panel",
          "size": [ "100%", 20 ],
          "offset": [ 2, 0 ],
          "orientation": "horizontal",
          "controls": [
            {
              "image": {
                "type": "image",
                "texture": "$button_icon",
                "size": [ 20, 20 ]
              }
            },
            {
              "padding": {
                "type": "panel",
                "size": [ 2, 0 ]
              }
            },
            {
              "label_panel": {
                "type": "panel",
                "size": [ "default", 20 ],
                "controls": [
                  {
                    "label": {
                      "type": "label",
                      "size": [ "default", 10 ],
                      "anchor_from": "left_middle",
                      "anchor_to": "left_middle",
                      "text": "$button_label",
                      "color": "$text_color"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "version_toggle_button_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "labels": {
          "type": "stack_panel",
          "size": [ "100%", 10 ],
          "offset": [ 2, 0 ],
          "orientation": "horizontal",
          "controls": [
            {
              "gotesupporter": {
                "type": "label",
                "text": "('Gote-Supporter ' + $version)",
                "color": "$text_color",
                "font_type": "#font_type",
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "font_property_panel",
                    "source_property_name": "#font_type",
                    "target_property_name": "#font_type"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "dropdown_chevron": {
          "type": "image",
          "texture": "textures/ui/chevron_white_down",
          "color": "$text_color",
          "anchor_from": "right_middle",
          "anchor_to": "right_middle",
          "offset": [ -5, 0 ],
          "size": [ 11, 11 ]
        }
      }
    ]
  },

  "faq_toggle_button_content": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "labels": {
          "type": "stack_panel",
          "size": [ "100%", 10 ],
          "offset": [ 2, 0 ],
          "orientation": "horizontal",
          "controls": [
            {
              "title": {
                "type": "label",
                "text": "('§　%' +$question + '.q')",
                "color": "$text_color",
                "font_type": "#font_type",
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "font_property_panel",
                    "source_property_name": "#font_type",
                    "target_property_name": "#font_type"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "dropdown_chevron": {
          "type": "image",
          "texture": "textures/ui/chevron_white_down",
          "color": "$text_color",
          "anchor_from": "right_middle",
          "anchor_to": "right_middle",
          "offset": [ -5, 0 ],
          "size": [ 11, 11 ]
        }
      }
    ]
  },

  "version_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "$version|default": "v1.0.0",
    "$details_controls|default": [],
    "controls": [
      {
        "toggle_button@common_toggles.dark_content_toggle": {
          "size": [ "100%", 20 ],
          "$button_content": "tabbed_upsell.version_toggle_button_content",
          "$toggle_name": "$version",
          "$toggle_view_binding_name": "$version",
          "$toggle_sound": true
        }
      },
      {
        "details_panel": {
          "type": "image",
          "size": [ "100%", "100%c + 5px" ],
          "texture": "textures/ui/menubackground",
          "controls": [
            {
              "stack": {
                "type": "stack_panel",
                "size": [ "100% - 4px", "100%c" ],
                "layer": 10,
                "controls": "$details_controls"
              }
            }
          ],
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "$version",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 0, 3 ]
        }
      }
    ]
  },

  "faq_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "$question|default": "",
    "controls": [
      {
        "toggle_button@common_toggles.dark_content_toggle": {
          "size": [ "100%", 20 ],
          "$button_content": "tabbed_upsell.faq_toggle_button_content",
          "$toggle_name": "$question",
          "$toggle_view_binding_name": "$question",
          "$toggle_sound": true
        }
      },
      {
        "details_panel": {
          "type": "image",
          "size": [ "100%", "100%c + 5px" ],
          "texture": "textures/ui/menubackground",
          "controls": [
            {
              "stack": {
                "type": "stack_panel",
                "size": [ "100% - 4px", "100%c" ],
                "layer": 10,
                "controls": [
                  {
                    "anser@tabbed_upsell.text": { "$text": "($question + '.a')" }
                  }
                ]
              }
            }
          ],
          "bindings": [
            {
              "binding_type": "view",
              "source_control_name": "$question",
              "source_property_name": "#toggle_state",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "padding_2": {
          "type": "panel",
          "size": [ 0, 3 ]
        }
      }
    ]
  },

  "text": {
    "type": "label",
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "size": [ "100%", "default" ],
    "shadow": true,
    "text": "$text",
    "$text|default": ""
  },

  "link_text@tabbed_upsell.text": {
    "size": [ "default", "default" ],
    "max_size": [ "100%", 10000 ],
    "font_type": "smooth",
    "color": "$2_color_format",
    "controls": [
      {
        "kasen": {
          "type": "image",
          "texture": "textures/ui/White",
          "color": "$2_color_format",
          "size": [ "100%", 1 ],
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle"
        }
      }
    ]
  },
  
  "playsound_play": {
    "type": "image",
    "texture": "textures/ui/playsound_play",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "playsound_stop": {
    "type": "image",
    "texture": "textures/ui/playsound_stop",
    "layer": 3,
    "size": [ 16, 16 ]
  },
  
  "play_sound_panel": {
    "type": "panel",
    "size": [ "100%", 32 ],
    "controls": [
      {
        "sound_panel": {
          "type": "image", 
          "size": [ "100%", 30 ],
          "texture": "textures/ui/White",
          "color": [ 0.1, 0.1, 0.1 ],
          "$sound_name|defalut": "dummy",
          "$sound_id|defalut": "pvp2.music.daisuke",
          "$sound_vol|defalut": 1.0,
          "$is_music|defalut": false,
          "controls": [
            {
              "flame": {
                "type": "image",
                "texture": "textures/ui/focus_border_white",
                "size": [ "100%", "100%" ],
                "color": [ 0, 0, 0 ]
              }
            },
            {
              "panel": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100% - 6px", 10 ],
                "layer": 1,
                "controls": [
                  {
                    "onpu": {
                      "type": "image",
                      "texture": "textures/ui/onpu",
                      "size": [ 10, 10 ]
                    }
                  },
                  {
                    "padding": {
                      "type": "panel",
                      "size": [ 2, 0 ]
                    }
                  },
                  {
                    "sound_name": {
                      "type": "label",
                      "size": [ "fill", 10 ],
                      "text": "$sound_name"
                    }
                  }
                ]
              }
            },
            {
              "play_stop": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100%c + 3px", 25 ],
                "anchor_from": "right_middle",
                "anchor_to": "right_middle",
                "controls": [
                  {
                    "play_button@common_buttons.dark_content_button": {
                      "size": [ 25, 25 ],
                      "$button_content": "tabbed_upsell.playsound_play",
                      "sound_name": "$sound_id",
                      "sound_volume": "$sound_vol",
                      "$pressed_button_name": "button.dummy"
                    }
                  },
                  {
                    "padding": {
                      "type": "panel",
                      "ignored": "(not $is_music)",
                      "size": [ 1, 0 ]
                    }
                  },
                  {
                    "stop_button@common_buttons.dark_content_button": {
                      "size": [ 25, 25 ],
                      "ignored": "(not $is_music)",
                      "$button_content": "tabbed_upsell.playsound_stop",
                      "sound_name": "$sound_id",
                      "sound_volume": 0.00000001,
                      "$pressed_button_name": "button.dummy"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "tab_selector@common_toggles.light_content_toggle": {
    "size": [ "100%", 30 ],
    "$toggle_name": "navigation_tab",
    "$radio_toggle_group": true,
    "$button_content": "tabbed_upsell.index_toggle_button_content"
  },
  
